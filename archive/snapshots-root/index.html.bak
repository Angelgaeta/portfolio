  <script>
    // DEBUG: s√©lection visible forc√©e (inline) pour les boutons data-fill
    (function () {
      function applySelected(btn) {
        // reset sur tous les boutons
        document.querySelectorAll("#quick-quote-form .qq-chip[data-fill]").forEach(b => {
          b.classList.remove("is-selected");
          b.setAttribute("aria-pressed", "false");
          b.style.background = "";
          b.style.borderColor = "";
          b.style.boxShadow = "";
          b.style.transform = "";
        });

        // active celui cliqu√©
        btn.classList.add("is-selected");
        btn.setAttribute("aria-pressed", "true");
        btn.style.background = "rgba(99,102,241,.28)";
        btn.style.borderColor = "rgba(99,102,241,.85)";
        btn.style.boxShadow = "0 0 0 3px rgba(99,102,241,.22), 0 16px 40px rgba(0,0,0,.35)";
        btn.style.transform = "translateY(-1px)";
      }

      // capture = m√™me si un autre script stopPropagation, on le voit quand m√™me
      document.addEventListener("click", function (e) {
        const btn = e.target.closest("#quick-quote-form .qq-chip[data-fill]");
        if (!btn) return;


        applySelected(btn);
      }, true);
    })();
  </script>

  <!-- JS Tabs + Wizard unlock Freelance + CDI validation -->
  <script>
    (function () {
      const root = document.getElementById('qq');
      const form = document.getElementById('quick-quote-form');
      if (!root || !form) return;

      const tabs = root.querySelectorAll('.qq-tab');
      const modeInput = document.getElementById('qq-mode');

      const title = document.getElementById('qq-title');
      const desc = document.getElementById('qq-desc');

      const panelFreelance = document.getElementById('qq-panel-freelance');
      const panelCDI = document.getElementById('qq-panel-cdi');

      // Freelance elements
      const email = document.getElementById('qq-email');
      const step2 = document.getElementById('qq-step2');
      const step3 = document.getElementById('qq-step3');
      const step2Status = document.getElementById('qq-step-2');
      const step3Status = document.getElementById('qq-step-3');
      const typeProject = document.getElementById('qq-type');
      const msg = document.getElementById('qq-msg');
      const submitFreelance = document.getElementById('qq-submit-freelance');
      const bar = document.getElementById('qq-progress-bar');
      const label = document.getElementById('qq-progress-label');

      // CDI elements
      const emailCDI = document.getElementById('qq-email-cdi');
      const role = document.getElementById('qq-role');
      const msgCDI = document.getElementById('qq-msg-cdi');
      const submitCDI = document.getElementById('qq-submit-cdi');

      function setTabUI(active) {
        tabs.forEach(t => {
          const on = t.dataset.mode === active;
          t.classList.toggle('is-active', on);
          t.setAttribute('aria-selected', on ? 'true' : 'false');
        });
      }

      function lock(el, statusEl) {
        el.classList.add('is-locked');
        el.classList.remove('is-open');
        if (statusEl) statusEl.textContent = 'Verrouill√©';
      }

      function unlock(el, statusEl) {
        el.classList.remove('is-locked');
        el.classList.add('is-open');
        if (statusEl) statusEl.textContent = '√Ä compl√©ter';
      }

      function setProgress(pct, txt) {
        if (bar) bar.style.width = pct + '%';
        if (label) label.textContent = txt;
      }

      function isEmailOK(input) {
        return input && input.value && input.checkValidity();
      }

      function syncFreelance() {
        // Step2 unlocked if email ok
        if (isEmailOK(email)) unlock(step2, step2Status);
        else lock(step2, step2Status);

        // Step3 unlocked if step2 ok
        if (isEmailOK(email) && typeProject && typeProject.value) unlock(step3, step3Status);
        else lock(step3, step3Status);

        // Progress + submit
        let stage = 1;
        if (isEmailOK(email)) stage = 2;
        if (isEmailOK(email) && typeProject && typeProject.value) stage = 3;

        if (stage === 1) setProgress(33, '1/3');
        if (stage === 2) setProgress(66, '2/3');
        if (stage === 3) setProgress(100, '3/3');

        submitFreelance.disabled = true;
        if (isEmailOK(email) && typeProject.value && msg.value.trim().length >= 5) {
          submitFreelance.disabled = false;
        }
      }

      function syncCDI() {
        submitCDI.disabled = true;
        if (isEmailOK(emailCDI) && role.value.trim().length >= 2 && msgCDI.value.trim().length >= 5) {
          submitCDI.disabled = false;
        }
      }

      function setMode(mode) {
        modeInput.value = mode;
        setTabUI(mode);

        const isCDI = mode === 'cdi';
        panelFreelance.hidden = isCDI;
        panelCDI.hidden = !isCDI;

        if (isCDI) {
          title.textContent = "Proposition de poste";
          desc.innerHTML = "Recruteur / entreprise : envoyez les infos cl√©s. R√©ponse sous <strong>24/48h</strong>.";
          // Copier l'email/pr√©nom si d√©j√† saisis (optionnel)
          if (email && email.value && emailCDI && !emailCDI.value) emailCDI.value = email.value;
          syncCDI();
          setTimeout(() => role.focus(), 30);
        } else {
          title.textContent = "Devis gratuit en 2 minutes";
          desc.innerHTML = "D√©crivez votre besoin. Je r√©ponds sous <strong>24/48h</strong> avec une estimation et la meilleure approche.";
          syncFreelance();
          setTimeout(() => email.focus(), 30);
        }
      }

      // Tabs
      tabs.forEach(t => t.addEventListener('click', () => setMode(t.dataset.mode)));

      // Freelance events
      email.addEventListener('input', syncFreelance);
      email.addEventListener('blur', syncFreelance);
      typeProject.addEventListener('change', syncFreelance);
      msg.addEventListener('input', syncFreelance);

      // CDI events
      emailCDI.addEventListener('input', syncCDI);
      emailCDI.addEventListener('blur', syncCDI);
      role.addEventListener('input', syncCDI);
      msgCDI.addEventListener('input', syncCDI);

      // Submit guard
      form.addEventListener('submit', (e) => {
        const mode = modeInput.value;
        if (mode === 'freelance') {
          // Use built-in validation
          if (!email.checkValidity() || !typeProject.value || msg.value.trim().length < 5) {
            e.preventDefault();
            email.reportValidity();
            typeProject.reportValidity();
            msg.reportValidity();
          }
        } else {
          if (!emailCDI.checkValidity() || role.value.trim().length < 2 || msgCDI.value.trim().length < 5) {
            e.preventDefault();
            emailCDI.reportValidity();
            role.reportValidity();
            msgCDI.reportValidity();
          }
        }
      });

      // Init
      setMode('freelance');
    })();
  </script>


  <script>
    document.addEventListener("click", (e) => {
      const a = e.target.closest('a[data-safe-out="1"]');
      if (!a) return;
      e.preventDefault();
      e.stopPropagation();
      window.open(a.href, "_blank", "noopener,noreferrer");
    }, true);

    // ===== Case Study Modal logic =====
    (function () {
      const overlay = document.getElementById('caseOverlay');
      const dialog = document.getElementById('caseDialog');
      const closeBtn = document.getElementById('caseClose');
      const content = document.getElementById('caseContent');
      const title = document.getElementById('caseTitle');

      let lastFocusedEl = null;

      function setAria(open) {
        overlay.setAttribute('aria-hidden', open ? 'false' : 'true');
      }

      function getFocusableElements(root) {
        const selectors = [
          'a[href]',
          'button:not([disabled])',
          'input:not([disabled])',
          'select:not([disabled])',
          'textarea:not([disabled])',
          '[tabindex]:not([tabindex="-1"])'
        ].join(',');
        return Array.from(root.querySelectorAll(selectors))
          .filter(el => el.offsetParent !== null);
      }

      // Transforme automatiquement un lien "d√©mo" en bouton styl√©
      function enhanceDemoLink() {
        // Cherche un lien qui contient "d√©mo" (texte)
        const links = Array.from(content.querySelectorAll('a[href]'));
        const demoLink = links.find(a => {
          const t = (a.textContent || '').toLowerCase();
          return t.includes('d√©mo') || t.includes('demo');
        });

        if (!demoLink) return;

        // Cr√©e une zone actions en bas si inexistante
        let actions = content.querySelector('.case-actions');
        if (!actions) {
          actions = document.createElement('div');
          actions.className = 'case-actions';
          content.appendChild(actions);
        }

        // Bouton principal "Voir la d√©mo"
        const btn = demoLink.cloneNode(true);
        btn.className = 'case-demo-btn';
        btn.innerHTML = '‚ñ∂ Voir la d√©mo';
        btn.target = '_blank';
        btn.rel = 'noopener noreferrer';

        // Option: bouton secondaire pour garder l'URL en clair
        // const secondary = demoLink.cloneNode(true);
        // secondary.className = 'case-demo-secondary';
        // secondary.textContent = 'Ouvrir dans un nouvel onglet';
        // secondary.target = '_blank';
        // secondary.rel = 'noopener noreferrer';

        // On enl√®ve le lien original pour √©viter doublon dans le texte
        demoLink.remove();

        actions.prepend(btn);
        // actions.appendChild(secondary);
      }

      function openCase(id) {
        const tpl = document.getElementById(id);
        if (!tpl) return;

        lastFocusedEl = document.activeElement;

        title.textContent = tpl.dataset.title || "Projet";
        content.innerHTML = tpl.innerHTML;

        enhanceDemoLink();

        overlay.style.display = 'flex';
        document.body.style.overflow = 'hidden';
        setAria(true);

        // Focus initial
        requestAnimationFrame(() => {
          dialog.focus();
          const focusables = getFocusableElements(dialog);
          (focusables[0] || dialog).focus();
        });
      }

      function closeCase() {
        overlay.style.display = 'none';
        document.body.style.overflow = '';
        setAria(false);
        content.innerHTML = '';

        if (lastFocusedEl && typeof lastFocusedEl.focus === 'function') {
          lastFocusedEl.focus();
        }
      }

      // Focus trap
      function trapFocus(e) {
        if (overlay.style.display !== 'flex') return;
        if (e.key !== 'Tab') return;

        const focusables = getFocusableElements(dialog);
        if (!focusables.length) {
          e.preventDefault();
          dialog.focus();
          return;
        }

        const first = focusables[0];
        const last = focusables[focusables.length - 1];

        if (e.shiftKey && document.activeElement === first) {
          e.preventDefault();
          last.focus();
        } else if (!e.shiftKey && document.activeElement === last) {
          e.preventDefault();
          first.focus();
        }
      }

      // Ouvrir depuis les cards du portfolio
      document.addEventListener('click', (e) => {
        const btn = e.target.closest('[data-case-open]');
        if (btn) {
          e.preventDefault();
          openCase(btn.getAttribute('data-case-open'));
          return;
        }
        // Click sur overlay (en dehors de la carte) => ferme
        if (e.target === overlay) closeCase();
      });

      closeBtn && closeBtn.addEventListener('click', closeCase);

      document.addEventListener('keydown', (e) => {
        if (overlay.style.display !== 'flex') return;

        if (e.key === 'Escape') {
          e.preventDefault();
          closeCase();
          return;
        }
        trapFocus(e);
      });
    })();
  </script>

  <!-- Scroll Animations -->
  <script>
    (function() {
      const observerOptions = {
        root: null,
        rootMargin: '0px 0px -50px 0px',
        threshold: 0.1
      };

      const observer = new IntersectionObserver((entries) => {
        entries.forEach(entry => {
          if (entry.isIntersecting) {
            entry.target.classList.add('is-visible');
            observer.unobserve(entry.target);
          }
        });
      }, observerOptions);

      document.querySelectorAll('.fade-in, .fade-in-left, .fade-in-right, .scale-in').forEach(el => {
        observer.observe(el);
      });

      // Lazy load images
      document.querySelectorAll('img[loading="lazy"]').forEach(img => {
        if (img.complete) {
          img.classList.add('loaded');
        } else {
          img.addEventListener('load', () => img.classList.add('loaded'));
        }
      });
    })();
  </script>

